{
  "name": "chonky-icon-fontawesome-custom",
  "version": "0.0.0",
  "description": "FontAwesome icon component for Chonky file browser",
  "license": "MIT",
  "main": "dist/index.iife.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsup src/index.ts --env.NODE_ENV development --format esm,cjs,iife --dts --watch --config tsup.config.ts",
    "build": "tsup src/index.ts --env.NODE_ENV production --format esm,cjs,iife --dts --config tsup.config.ts",
    "prepare": "tsup src/index.ts --env.NODE_ENV production --format esm,cjs,iife --dts --config tsup.config.ts",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "peerDependencies": {
    "react": "^16.0.0 || ^17.0.0 || ^18.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsup src/index.ts --env.NODE_ENV production --format esm,cjs,iife --dts --config tsup.config.ts"
    }
  },
  "module": "dist/index.esm.js",
  "size-limit": [
    {
      "path": "dist/index.cjs.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/index.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^4.10.1",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.2",
    "husky": "^5.1.3",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "size-limit": "^4.10.1",
    "tsup": "^6.5.0",
    "tslib": "^2.4.1",
    "typescript": "^4.6.4"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.32",
    "@fortawesome/free-brands-svg-icons": "5.13.1",
    "@fortawesome/free-solid-svg-icons": "^5.13.1",
    "@fortawesome/react-fontawesome": "^0.1.12",
    "chonky": "^2.3.2"
  },
  "homepage": "https://github.com/akbaruddin/Chonky",
  "author": {
    "name": "akbaruddin"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/akbaruddin/Chonky.git"
  },
  "bugs": {
    "url": "https://github.com/akbaruddin/Chonky/issues"
  },
  "keywords": [
    "file",
    "chonky",
    "file-dialog",
    "file-browser",
    "react-component"
  ]
}
